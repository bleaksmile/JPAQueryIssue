package issue;

import java.util.UUID;-query-issue;

@Data
@Entity
@Table(name = "STUDENT")
public class Teacher{

  @Id
  @Type(type = UUID_TYPE)
  @Column(name = "ID", nullable = false, unique = true, updatable = false)
  @Nonnull
  private UUID id;

  @Column(name="TEACHER_NAME")
  private String teacherName;
}